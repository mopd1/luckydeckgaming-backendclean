    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "fs": "^0.0.1-security",
    "google-auth-library": "^9.15.0",
    "googleapis": "^148.0.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.4.0",
    "mongoose": "^8.9.3",
    "multer": "^1.4.5-lts.2",
